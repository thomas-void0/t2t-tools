#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thomas/Documents/code/typescript-type-tools/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@5.0.0/node_modules/conventional-commits-parser/node_modules:/Users/thomas/Documents/code/typescript-type-tools/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@5.0.0/node_modules:/Users/thomas/Documents/code/typescript-type-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/thomas/Documents/code/typescript-type-tools/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@5.0.0/node_modules/conventional-commits-parser/node_modules:/Users/thomas/Documents/code/typescript-type-tools/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@5.0.0/node_modules:/Users/thomas/Documents/code/typescript-type-tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-commits-parser/cli.mjs" "$@"
else
  exec node  "$basedir/../conventional-commits-parser/cli.mjs" "$@"
fi
